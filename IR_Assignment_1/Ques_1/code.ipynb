    "\n",
    "#Taking string as input and changing the whole string to lowercase\n",
    "def change_to_lowercase(file_data):\n",
    "    return(file_data.lower())\n",
    "\n",
    "#Taking string as inout and returning the list of tokens\n",
    "def perform_tokenization(file_data):\n",
    "    return(nltk.word_tokenize(file_data))\n",
    "\n",
    "#Taking input as list of tokens and remove all the english stopwords\n",
    "def remove_stopwords(file_data):\n",
    "    english_stopwords = stopwords.words(\"english\")\n",
    "    # tokens=perform_tokenization(file_data)\n",
    "    tokens_wo_stopwords = [t for t in file_data if t not in english_stopwords]\n",
    "    # print(type(tokens_wo_stopwords))\n",
    "    return tokens_wo_stopwords\n",
    "    \n",
    "#Taking input as list of tokens and remove all the punctuations  \n",
    "def remove_punctuations(file_data):\n",
    "    lst=[]\n",
    "    for i in file_data:\n",
    "        if(i.isalpha()):\n",
    "            lst.append(i)   \n",
    "    return lst\n",
    "        \n",
    "\n",
    "def read_text_file(file_path,i):\n",
    "    with open(file_path, 'r') as f:\n",
    "        print()\n",
    "        file_data=f.read()\n",
    "        if(i<=5):\n",
    "            before.append(file_data)\n",
    "        print()\n",
    "        \n",
    "        file_data=change_to_lowercase(file_data)\n",
    "        if(i<=5):\n",
    "            after.append(file_data)\n",
    "            before.append(file_data)\n",
    "        print()\n",
    "        \n",
    "        file_data=perform_tokenization(file_data)\n",
    "        if(i<=5):\n",
    "            after.append(\" \".join(file_data))\n",
    "            before.append(\" \".join(file_data))\n",
    "        print()\n",
    "        \n",
    "        file_data=remove_stopwords(file_data)\n",
    "        if(i<=5):\n",
    "            after.append(\" \".join(file_data))\n",
    "            before.append(\" \".join(file_data))\n",
    "        print()\n",
    "        \n",
    "        file_data=remove_punctuations(file_data)\n",
    "        if(i<=5):\n",
    "            after.append(\" \".join(file_data))\n",
    "            before.append(\" \".join(file_data))\n",
    "        print()\n",
    "        \n",
    "        if(i<=5):\n",
    "            after.append(\" \".join(file_data))\n",
    "\n",
    "    return file_data\n",
    "    "
