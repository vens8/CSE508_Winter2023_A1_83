    "def change_text(file_data):\n",
    "    \n",
    "    soup = BeautifulSoup(file_data, 'html.parser')\n",
    "    \n",
    "    #Extracting the content inside title tag and returning the object of bs4\n",
    "    div_bs4_1_title = soup.find('title')\n",
    "    \n",
    "    #Replacing all new line characters with a space\n",
    "    title=div_bs4_1_title.string.replace('\\n', \" \")\n",
    "    \n",
    "    #Extracting the content inside title tag and returning the object of bs4\n",
    "    div_bs4_1_text = soup.find('text')\n",
    "    \n",
    "    #Replacing all new line characters with a space\n",
    "    text=div_bs4_1_text.string.replace('\\n', \" \")\n",
    "    \n",
    "    #COmbining title and text for each document content with a blank space\n",
    "    return(title+text) \n",
    "\n",
    "\n",
    "#Reading a file in directory \n",
    "def read_text_file(file_path):\n",
    "    changed_text=\"\"\n",
    "    with open(file_path, 'r') as f:\n",
    "        changed_text=change_text(f.read())\n",
    "        print()\n",
    "        print()\n",
    "        \n",
    "    #Write the changed processed data to the dataset file\n",
    "    with open(file_path, 'w') as f:\n",
    "        f.writelines(changed_text)"
